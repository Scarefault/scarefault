#!/bin/bash

RESULT_FILE="../../log/scarefault.log"
EXPECT_FILE=$1

iterator=1

cat $RESULT_FILE | cut -f2-3 | while read line; do
  if [ "$line" = "$(sed "${iterator}q;d" $EXPECT_FILE)" ]; then
    echo "[OK]" >> out
  else
    echo "[FAIL]" >> out
  fi

  let iterator=iterator+1
done

cat out -n
rm out
